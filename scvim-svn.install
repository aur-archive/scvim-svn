post_install() {
echo "If you want help from within scvim run:"
echo "scvim_make_help -c -s /usr/share/SuperCollider/Help"
echo ""
echo "For more options and for usage read:"
echo "https://supercollider.svn.sourceforge.net/svnroot/supercollider/trunk/editors/scvim/README"
}


post_upgrade() {
post_install $1
}


